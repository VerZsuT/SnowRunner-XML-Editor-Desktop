{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "outDir": "dist",
    "newLine": "LF",
    "baseUrl": ".",
    "strict": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "paths": {
      "#types": [
        "../types/index"
      ],
      "#types/*": [
        "../types/*"
      ],
      "#consts": [
        "../consts"
      ],
      "#enums": [
        "../enums/index"
      ],
      "#enums/*": [
        "../enums/*"
      ],
      "#globalTexts/*": [
        "../globalTexts/*"
      ],
      "#images/*": [
        "../images/*"
      ],
      "#r-scripts/*": [
        "scripts/*"
      ],
      "#services": [
        "services/index"
      ],
      "#services/*": [
        "services/*"
      ],
      "#components/*": [
        "components/*"
      ],
      "#helpers/*": [
        "helpers/*"
      ],
      "#hooks/*": [
        "hooks/*"
      ],
      "#pages/*": [
        "pages/*"
      ],
      "#templates": [
        "templates/index"
      ],
      "#templates/*": [
        "templates/*"
      ],
      "#r/*": [
        "./*"
      ]
    }
  },
  "include": [
    "./**/*",
    "../global.d.ts"
  ],
  "exclude": ["node_modules"]
}
