{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["ESNext"],
    "outDir": "dist",
    "newLine": "LF",
    "baseUrl": ".",
    "strict": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "paths": {
      "#types": [
        "../types/index"
      ],
      "#types/*": [
        "../types/*"
      ],
      "#consts": [
        "../consts"
      ],
      "#enums": [
        "../enums/index"
      ],
      "#enums/*": [
        "../enums/*"
      ],
      "#globalTexts/*": [
        "../globalTexts/*"
      ],
      "#images/*": [
        "../images/*"
      ],
      "#classes": [
        "classes/index"
      ],
      "#classes/*": [
        "classes/*"
      ],
      "#m-scripts": [
        "scripts/index"
      ],
      "#m-scripts/*": [
        "scripts/*"
      ],
      "#windows": [
        "windows/index"
      ],
      "#windows/*": [
        "windows/*"
      ],
      "#m/*": [
        "./*"
      ]
    }
  },
  "exclude": ["node_modules"]
}
