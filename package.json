{
	"name": "snowrunner-xml-editor-desktop",
	"productName": "SnowRunner XML Editor",
	"version": "0.7.0",
	"description": "Allows you to visually edit the game's XML files.",
	"repository": {
		"type": "git",
		"url": "https://github.com/VerZsuT/SnowRunner-XML-Editor-Desktop.git"
	},
	"main": ".webpack/main",
	"scripts": {
		"build": "node ./build_scripts/preBuild.js && npm run package-prod && node ./build_scripts/postBuild.js x32",
		"build-x64": "node ./build_scripts/preBuild.js && npm run package-prod-x64 && node ./build_scripts/postBuild.js x64",
		"package": "electron-forge package --platform=win32 --arch=ia32",
		"package-prod": "cross-env NODE_ENV=production electron-forge package --platform=win32 --arch=ia32",
		"package-prod-x64": "cross-env NODE_ENV=production electron-forge package --platform=win32 --arch=x64",
		"package-x64": "electron-forge package --platform=win32 --arch=x64",
		"start": "cross-env NODE_ENV=development electron-forge start",
		"lint": "eslint src/**",
		"lint:fix": "npm run lint -- --fix"
	},
	"keywords": [],
	"author": {
		"name": "VerZsuT",
		"email": "verzsut@gmail.com"
	},
	"license": "MIT",
	"config": {
		"forge": "./configs/forge.js"
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.0.0-beta.64",
		"@electron-forge/plugin-webpack": "^6.0.0-beta.64",
		"@types/memoizee": "^0.4.8",
		"@types/react": "^18.0.14",
		"@types/react-dom": "^18.0.5",
		"@types/react-window": "^1.8.5",
		"@typescript-eslint/eslint-plugin": "^5.29.0",
		"@typescript-eslint/parser": "^5.29.0",
		"@vercel/webpack-asset-relocator-loader": "^1.7.2",
		"copy-webpack-plugin": "^11.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^6.7.1",
		"electron": "^19.0.5",
		"eslint": "^8.18.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-react": "^7.30.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"fork-ts-checker-webpack-plugin": "^7.2.11",
		"node-loader": "^2.0.0",
		"sass": "^1.52.3",
		"sass-loader": "^13.0.0",
		"style-loader": "^3.3.1",
		"translate": "^1.4.1",
		"ts-loader": "^9.3.0",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"@ant-design/icons": "^4.7.0",
		"@emotion/react": "^11.9.3",
		"@emotion/styled": "^11.9.3",
		"@mui/icons-material": "^5.8.4",
		"@mui/material": "^5.8.5",
		"@reduxjs/toolkit": "^1.8.2",
		"antd": "^4.21.3",
		"cheerio": "^1.0.0-rc.11",
		"memoizee": "^0.4.15",
		"mui-nested-menu": "^2.0.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^8.0.2",
		"react-window": "^1.8.7"
	}
}
