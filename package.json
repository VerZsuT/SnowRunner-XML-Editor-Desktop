{
	"name": "snowrunner-xml-editor-desktop",
	"productName": "SnowRunner XML Editor",
	"version": "0.7.4a",
	"description": "Allows you to visually edit the game's XML files.",
	"repository": {
		"type": "git",
		"url": "https://github.com/VerZsuT/SnowRunner-XML-Editor-Desktop.git"
	},
	"main": ".webpack/main",
	"scripts": {
		"build": "npm run i:electron:latest && npm run build:before && npm run package-prod && npm run build:after",
		"build:legacy": "npm run i:electron:legacy && npm run build:before -- legacy && npm run package-prod && npm run build:after -- legacy",
		"build:before": "ts-node ./build_scripts/before.ts",
		"build:after": "ts-node ./build_scripts/after.ts",
		"package": "electron-forge package --platform=win32 --arch=ia32",
		"package-prod": "cross-env NODE_ENV=production electron-forge package --platform=win32 --arch=ia32",
		"start": "cross-env NODE_ENV=development electron-forge start",
		"lint": "eslint src/**",
		"lint:fix": "npm run lint -- --fix",
		"check": "tsc --project src/tsconfig.json --noEmit",
		"i:electron:latest": "npm i electron@latest --save-dev",
		"i:electron:legacy": "npm i electron@22 --save-dev"
	},
	"keywords": [],
	"author": {
		"name": "VerZsuT",
		"email": "verzsut@gmail.com"
	},
	"license": "MIT",
	"config": {
		"forge": "./configs/forge.js"
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.2.1",
		"@electron-forge/plugin-webpack": "^6.2.1",
		"@types/memoizee": "^0.4.8",
		"@types/react": "^18.2.13",
		"@types/react-dom": "^18.2.6",
		"@types/react-window": "^1.8.5",
		"@typescript-eslint/eslint-plugin": "^5.60.0",
		"@typescript-eslint/parser": "^5.60.0",
		"copy-webpack-plugin": "^11.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^6.8.1",
		"electron": "^25.1.1",
		"eslint": "^8.43.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-react": "^7.32.2",
		"node-loader": "^2.0.0",
		"sass": "^1.63.4",
		"sass-loader": "^13.3.2",
		"style-loader": "^3.3.3",
		"ts-loader": "^9.4.3",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.3"
	},
	"dependencies": {
		"@ant-design/icons": "^5.1.4",
		"@ant-design/pro-components": "^2.6.2",
		"@reduxjs/toolkit": "^1.9.5",
		"antd": "^5.6.2",
		"cheerio": "^1.0.0-rc.12",
		"emr-bridge": "^1.5.0",
		"memoizee": "^0.4.15",
		"react": "^18.2.0",
		"react-afc": "^3.3.3",
		"react-dom": "^18.2.0",
		"react-redux": "^8.1.0",
		"react-window": "^1.8.9",
		"tslib": "^2.5.3"
	}
}