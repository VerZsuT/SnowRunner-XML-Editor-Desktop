{
	"name": "snowrunner-xml-editor-desktop",
	"productName": "SnowRunner XML Editor",
	"version": "0.6.6",
	"description": "Allows you to visually edit the game's XML files.",
	"repository": {
		"type": "git",
		"url": "https://github.com/VerZsuT/SnowRunner-XML-Editor-Desktop.git"
	},
	"main": ".webpack/main",
	"scripts": {
		"start": "electron-forge start",
		"build": "node ./build_scripts/preBuild.js && npm run package-prod && node ./build_scripts/postBuild.js",
		"package-prod": "cross-env NODE_ENV=production electron-forge package --platform=win32 --arch=ia32",
		"package": "electron-forge package --platform=win32 --arch=ia32"
	},
	"keywords": [],
	"author": {
		"name": "VerZsuT",
		"email": "verzsut@gmail.com"
	},
	"license": "MIT",
	"config": {
		"forge": "./configs/forge.js"
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.0.0-beta.61",
		"@electron-forge/plugin-webpack": "^6.0.0-beta.61",
		"@types/react": "^17.0.37",
		"@types/react-dom": "^17.0.11",
		"@vercel/webpack-asset-relocator-loader": "^1.7.0",
		"cheerio": "^1.0.0-rc.10",
		"copy-webpack-plugin": "^9.0.1",
		"cross-env": "^7.0.3",
		"css-loader": "^6.3.0",
		"electron": "^16.0.0",
		"fork-ts-checker-webpack-plugin": "^6.3.4",
		"node-loader": "^2.0.0",
		"style-loader": "^3.3.0",
		"ts-loader": "^9.2.6",
		"typescript": "^4.4.4"
	},
	"dependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	}
}