{
	"name": "snowrunner-xml-editor-desktop",
	"productName": "SnowRunner XML Editor",
	"version": "0.7.3",
	"description": "Allows you to visually edit the game's XML files.",
	"repository": {
		"type": "git",
		"url": "https://github.com/VerZsuT/SnowRunner-XML-Editor-Desktop.git"
	},
	"main": ".webpack/main",
	"scripts": {
		"build": "npm run build:before && npm run package-prod && npm run build:after",
		"build:before": "ts-node ./build_scripts/before.ts",
		"build:after": "ts-node ./build_scripts/after.ts x32",
		"package": "electron-forge package --platform=win32 --arch=ia32",
		"package-prod": "cross-env NODE_ENV=production electron-forge package --platform=win32 --arch=ia32",
		"start": "cross-env NODE_ENV=development electron-forge start",
		"lint": "eslint src/**",
		"lint:fix": "npm run lint -- --fix",
		"check": "tsc --project src/tsconfig.json --noEmit"
	},
	"keywords": [],
	"author": {
		"name": "VerZsuT",
		"email": "verzsut@gmail.com"
	},
	"license": "MIT",
	"config": {
		"forge": "./configs/forge.js"
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.1.1",
		"@electron-forge/plugin-webpack": "^6.1.1",
		"@types/memoizee": "^0.4.8",
		"@types/react": "^18.2.6",
		"@types/react-dom": "^18.2.4",
		"@types/react-window": "^1.8.5",
		"@typescript-eslint/eslint-plugin": "^5.59.6",
		"@typescript-eslint/parser": "^5.59.6",
		"@vercel/webpack-asset-relocator-loader": "^1.7.3",
		"copy-webpack-plugin": "^11.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^6.7.3",
		"electron": "^24.3.0",
		"eslint": "^8.40.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-react": "^7.32.2",
		"fork-ts-checker-webpack-plugin": "^8.0.0",
		"node-loader": "^2.0.0",
		"sass": "^1.62.1",
		"sass-loader": "^13.2.2",
		"style-loader": "^3.3.2",
		"ts-loader": "^9.4.2",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4"
	},
	"dependencies": {
		"@ant-design/icons": "^5.0.1",
		"@ant-design/pro-components": "^2.4.15",
		"@reduxjs/toolkit": "^1.9.5",
		"antd": "^5.5.0",
		"cheerio": "^1.0.0-rc.12",
		"emr-bridge": "^1.2.0",
		"memoizee": "^0.4.15",
		"rc-util": "^5.30.0",
		"react": "^18.2.0",
		"react-afc": "^3.3.3",
		"react-dom": "^18.2.0",
		"react-redux": "^8.0.5",
		"react-window": "^1.8.9"
	}
}