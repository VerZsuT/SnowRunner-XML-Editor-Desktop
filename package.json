{
	"name": "snowrunner-xml-editor-desktop",
	"productName": "SnowRunner XML Editor",
	"version": "0.7.2",
	"description": "Allows you to visually edit the game's XML files.",
	"repository": {
		"type": "git",
		"url": "https://github.com/VerZsuT/SnowRunner-XML-Editor-Desktop.git"
	},
	"main": ".webpack/main",
	"scripts": {
		"build": "node ./build_scripts/before.js && npm run package-prod && node ./build_scripts/after.js x32",
		"package": "electron-forge package --platform=win32 --arch=ia32",
		"package-prod": "cross-env NODE_ENV=production electron-forge package --platform=win32 --arch=ia32",
		"start": "cross-env NODE_ENV=development electron-forge start",
		"lint": "eslint src/**",
		"lint:fix": "npm run lint -- --fix",
		"check": "tsc --project src/tsconfig.json --noEmit"
	},
	"keywords": [],
	"author": {
		"name": "VerZsuT",
		"email": "verzsut@gmail.com"
	},
	"license": "MIT",
	"config": {
		"forge": "./configs/forge.js"
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.0.4",
		"@electron-forge/plugin-webpack": "^6.0.4",
		"@types/memoizee": "^0.4.8",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.10",
		"@types/react-window": "^1.8.5",
		"@typescript-eslint/eslint-plugin": "^5.47.1",
		"@typescript-eslint/parser": "^5.47.1",
		"@vercel/webpack-asset-relocator-loader": "^1.7.3",
		"copy-webpack-plugin": "^11.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^6.7.3",
		"electron": "^22.0.0",
		"electron-devtools-installer": "^3.2.0",
		"eslint": "^8.30.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-react": "^7.31.11",
		"fork-ts-checker-webpack-plugin": "^7.2.14",
		"node-loader": "^2.0.0",
		"sass": "^1.57.1",
		"sass-loader": "^13.2.0",
		"style-loader": "^3.3.1",
		"ts-loader": "^9.4.2",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"@ant-design/icons": "^4.8.0",
		"@ant-design/pro-components": "^2.3.47",
		"@reduxjs/toolkit": "^1.9.1",
		"antd": "^5.1.1",
		"cheerio": "^1.0.0-rc.12",
		"emr-bridge": "^1.1.2",
		"memoizee": "^0.4.15",
		"react": "^18.2.0",
		"react-afc": "^3.1.0",
		"react-dom": "^18.2.0",
		"react-redux": "^8.0.5",
		"react-window": "^1.8.8"
	}
}
